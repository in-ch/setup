{
  "compilerOptions": {
      "module": "commonjs",
      "target": "es6",
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "noEmitHelpers": true,
      "noEmitOnError": true,
      "skipLibCheck": true,
      "lib": [
          "es6",
          "dom",
          "es2017"
      ],
      "baseUrl": ".",
  },
  "exclude": ["__tests__", "node_modules", "dist"]
}