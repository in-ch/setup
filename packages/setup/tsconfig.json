{
  "compilerOptions": {
    "outDir": "./dist",
    "target": "ESNext",
    "strict": true,
    "esModuleInterop": true,
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "skipLibCheck": true,
    "noUncheckedIndexedAccess": true,
    "exactOptionalPropertyTypes": true,
    "allowImportingTsExtensions": true,
    "declaration": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "lib/*": ["lib/*"]
    }
  },
}